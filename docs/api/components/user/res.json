{
  "LoginAndTokenGeneration": {
    "type": "object",
    "properties": {
      "message": {
        "type": "string",
        "example": "Token is generated, Login Successful."
      },
      "token": {
        "type": "string",
        "example": "eyJpZCI6M5hbWUiOiJ"
      }
    }
  },
  "getEnrollmentsStudent": {
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "enrollId": {
          "type": "integer",
          "example": 6
        },
        "enrollAt": {
          "type": "timestamp",
          "example": "2023-05-14T18:30:00.000Z"
        },
        "studentId": {
          "type": "integer",
          "example": 57
        },
        "courseId": {
          "type": "integer",
          "example": 57
        }
      }
    }
  }
}
